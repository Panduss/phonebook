{
  "compilerOptions":{
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "node",
    "experimentalDecorators":true,
    "esModuleInterop": true,
    "typeRoots":[
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ]
  },
  "include": [
    "./src",
    "./test"
  ],
  "exclude":[
    "node_modules"
  ]
}